tag: {{ _venv_git_repo_upstream_tag_result.stdout | default('No upstream tag(s)', true) }}
upper-constraints: {{ _venv_git_repo_constraint_tag_result.stdout | default('No upper constraint tag(s)', true) }}

# If a change can corrupt the wheelhouse cache we can bump this value and this will rebuild
# the cache during CI and the next change on this host will clear the wheelhouse out, so that
# we can move a change through CI.
cache_invalidation_version: 2
