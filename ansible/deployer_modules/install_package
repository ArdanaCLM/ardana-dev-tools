# WANT_JSON
(
date
echo install_package:
cat "$1"
echo
) >> /var/log/install_package.log
unset http_proxy https_proxy no_proxy
unset HTTP_PROXY HTTPS_PROXY NO_PROXY
exec /opt/stack/service/packager/venv/bin/install_package "$@"
